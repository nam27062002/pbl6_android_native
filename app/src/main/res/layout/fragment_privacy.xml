<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#ffffff"
    android:theme="@android:style/Theme.Light"
    tools:context=".PrivacyFragment">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/top_change"
        android:layout_width="match_parent"
        android:layout_height="70dp"
        android:background="@drawable/rectangle_8"
        tools:ignore="MissingConstraints"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:layout_marginTop="0dp">
        <Button
            android:id="@+id/bt_change"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:background="@drawable/btleft"

            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            android:layout_marginStart="16dp"
            android:layout_marginTop="11dp"
            android:layout_marginBottom="13dp"
            tools:ignore="NotSibling" />

    </androidx.constraintlayout.widget.ConstraintLayout>
    <WebView
        android:id="@+id/web_view"
        android:layout_height="0dp"
        android:layout_width="0dp"
        android:layout_marginBottom="80dp"
        app:layout_constraintTop_toBottomOf="@id/top_change"
        app:layout_constraintLeft_toLeftOf="@id/top_change"
        app:layout_constraintRight_toRightOf="@id/top_change"
        app:layout_constraintBottom_toBottomOf="parent"
        android:mimeType="text/html"/>

</androidx.constraintlayout.widget.ConstraintLayout>