    </style>
<!--    _________________________-->
    <style name="txt_change">
        <item name="android:textSize">20sp</item>
        <item name="android:textColor">@color/white</item>
        <item name="android:fontFamily">@font/PTSans_Bold</item>
    </style>
    <style name="tittle">
        <item name="android:textColor">#3E4958</item>
        <item name="android:textSize">13sp</item>
        <item name="android:fontFamily">@font/PTSans_Bold</item>
    </style>
    <style name="txt_fail">
        <item name="android:textColor">#FD4B4B</item>
        <item name="android:textSize">13sp</item>
        <item name="android:fontFamily">@font/Poppins_Bold</item>
    </style>
    <style name="txt_holder">
        <item name="android:textColor">#D0D0D0</item>
